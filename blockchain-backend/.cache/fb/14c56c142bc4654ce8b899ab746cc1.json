riginal":{"line":41,"column":12}},{"generated":{"line":105,"column":22},"source":"components/Block.js","original":{"line":40,"column":8}},{"generated":{"line":106,"column":0},"source":"components/Block.js","original":{"line":46,"column":5}},{"name":"Component","generated":{"line":110,"column":2},"source":"components/Block.js","original":{"line":5,"column":20}},{"name":"Block","generated":{"line":112,"column":15},"source":"components/Block.js","original":{"line":64,"column":15}}],"sources":{"components/Block.js":"import React, { Component } from 'react';\r\nimport {Button} from 'react-bootstrap';\r\nimport Transaction from './Transaction';\r\n\r\nclass Block extends Component{\r\n\r\n    state = {\r\n        displayTransaction : false\r\n    }\r\n\r\n    toggleDisplayTransaction = () =>{\r\n        this.setState({\r\n            displayTransaction: !this.state.displayTransaction\r\n        })\r\n    }\r\n\r\n    get displayTransaction (){\r\n        const {data} = this.props.block;\r\n\r\n        const stringifiedData = JSON.stringify(data);\r\n        const dataDisplay = stringifiedData.length >35 ? `${stringifiedData.substring(0,35)}` : stringifiedData;\r\n        \r\n        if(this.state.displayTransaction){\r\n            return(\r\n                <div>\r\n                    {\r\n                        data.map(transaction =>(\r\n                            <div key= {transaction.id}>\r\n                                <hr/>\r\n                                <Transaction transaction = {transaction}/>\r\n                            </div>\r\n                        ))\r\n                    }\r\n                    <br/>\r\n                    <Button className=\"ToggleButton\" onClick={this.toggleDisplayTransaction}>Show Less</Button>\r\n                </div>\r\n            )\r\n        }\r\n\r\n        return (\r\n            <div>\r\n                <div>Data: {dataDisplay}</div>\r\n                <Button variant=\"danger\" size=\"sm\" style={ {textAlign : \"center\"}} onClick={this.toggleDisplayTransaction}>Show More</Button>\r\n            </div>\r\n        )\r\n    }\r\n\r\n    render(){\r\n\r\n        const {hash, timeStamp} = this.props.block;\r\n\r\n        const hashDisplay = `${hash.substring(0,15)}...`; \r\n\r\n        return(\r\n            <div className=\"Block\">\r\n                <div>Hash : {hashDisplay}</div>\r\n                <div>Timestamp : {new Date(timeStamp).toLocaleString()}</div>\r\n                {this.displayTransaction}\r\n            </div>\r\n        )\r\n    }\r\n}\r\n\r\nexport default Block;"},"lineCount":null}},"error":null,"hash":"ba3c1c0ea236e3dea85b79ddd9d172cb","cacheData":{"env":{}}}